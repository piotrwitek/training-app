{
  "name": "training-management-tool",
  "version": "0.2.0",
  "description": "training-management-tool",
  "main": "server.js",
  "repository": {
    "url": "https://github.com/piotrwitek/training-management-tool"
  },
  "scripts": {
    "start": "start npm run server",
    "server": "node server.js",
    "postinstall": "npm run install-jspm && npm run install-typings",
    "install-jspm": "./node_modules/.bin/jspm install",
    "install-typings": "./node_modules/.bin/typings install",
    "dev-bundle": "./node_modules/.bin/jspm bundle dev-bundle.ts dev-bundle.js -id",
    "dev-unbundle": "./node_modules/.bin/jspm unbundle",
    "build": "./node_modules/.bin/jspm bundle src/app out/app.transpiled.js && npm run regenerator && npm run copy-app && npm run copy-system",
    "build-min": "./node_modules/.bin/jspm bundle src/app out/app.transpiled.js --minify --skip-source-maps && npm run regenerator && npm run copy-app && npm run copy-system",
    "regenerator": "cd out && regenerator -r app.transpiled.js > app.regenerator.js",
    "copy-app": "cp out/app.regenerator.js dist/app.js",
    "copy-system": "cp jspm_packages/system.js dist/system.js && cp jspm.config.js dist/jspm.config.js",
    "init-deploy": "mkdir dist && git clone https://github.com/piotrwitek/training-management-tool --branch gh-pages dist",
    "deploy": "cd dist && git add --all && git commit -m \"New Release\" && git push",
    "uglify": "cd dist && uglifyjs app.js -o app.min.js --screw-ie8 && cd ..",
    "docs": "./node_modules/.bin/typedoc src/ --out docs/ --module system --jsx react --target es6"
  },
  "author": "Piotr Witek <piotrek.witek@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chokidar-socket-emitter": "^0.5.2",
    "http-server": "^0.9.0",
    "jspm": "^0.17.0-beta.22",
    "jspm-server": "^1.0.1",
    "typedoc": "^0.4.3",
    "typings": "^1.3.0"
  },
  "jspm": {
    "name": "training-management-tool",
    "main": "app.tsx",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "mobx-react": "npm:mobx-react@^3.3.1",
      "react-dom": "npm:react-dom@^15.1.0"
    },
    "devDependencies": {
      "css": "github:systemjs/plugin-css@^0.1.23",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.16",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "mobx": "npm:mobx@^2.1.7",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "react": "npm:react@^15.1.0",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
